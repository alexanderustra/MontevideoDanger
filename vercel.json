{
  "version": 2,
  "builds": [
    {
      "src": "app/app.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app/app.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
